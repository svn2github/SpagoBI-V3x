<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="it.eng.spagobi.commons.metadata.SbiBinContents" table="SBI_BINARY_CONTENTS">
        <id name="id" type="integer">
            <column name="BIN_ID" />
   <generator class="org.hibernate.id.enhanced.TableGenerator">
		<param name="optimizer">hilo</param>
		<param name="segment_value">SBI_BINARY_CONTENTS</param>
		<param name="initial_value">1</param>		
   </generator>
        </id>
        <property name="content" type="binary">
            <column name="BIN_CONTENT" sql-type="longvarbinary"/>
        </property>
    </class>
</hibernate-mapping>
