<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="it.eng.spagobi.commons.metadata.SbiBinContents" table="SBI_BINARY_CONTENTS">
        <id name="id" type="integer">
            <column name="BIN_ID" />
            <generator class="native">
           		<param name="sequence">sbi_binary_contents_seq</param>
            </generator>
        </id>
        <property name="content" type="binary">
            <column name="BIN_CONTENT" sql-type="longvarbinary"/>
        </property>
    </class>
</hibernate-mapping>
