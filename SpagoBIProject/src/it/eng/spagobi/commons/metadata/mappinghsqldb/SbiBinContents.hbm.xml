<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="it.eng.spagobi.commons.metadata.SbiBinContents" table="sbi_binary_contents">
        <id name="id" type="integer">
            <column name="bin_id" />
            <generator class="native">
            	<param name="sequence">sbi_binary_seq</param>
            </generator>
        </id>
        <property name="content" type="binary">
            <column name="bin_content" sql-type="longvarbinary"/>
        </property>
    </class>
</hibernate-mapping>
