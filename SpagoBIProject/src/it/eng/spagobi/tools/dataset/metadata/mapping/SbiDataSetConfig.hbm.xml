<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="it.eng.spagobi.tools.dataset.metadata.SbiDataSetConfig" table="SBI_DATA_SET" lazy="false">
        <id name="dsId" type="int">
            <column name="DS_ID" />     
   <generator class="org.hibernate.id.enhanced.TableGenerator">
		<param name="optimizer">hilo</param>
		<param name="table_name">sbi_index</param>
		<param name="segment_value">sbi_data_set_seq</param>	
		<param name="initial_value">1</param>	
   </generator>                
        </id>     
        <property name="name" type="string">
            <column name="NAME" />
        </property>
        <property name="description" type="string">
            <column name="DESCR" />
        </property>
        <property name="label" type="string">
            <column name="LABEL" />
        </property>
        <property name="userIn" type="string">
            <column name="USER_IN" not-null="true" />
        </property>
        <property name="userUp" type="string">
            <column name="USER_UP" />
        </property>
        <property name="userDe" type="string">
            <column name="USER_DE" />
        </property>
        <property name="sbiVersionIn" type="string">
            <column name="SBI_VERSION_IN" />
        </property>
        <property name="sbiVersionUp" type="string">
            <column name="SBI_VERSION_UP" />
        </property>
        <property name="sbiVersionDe" type="string">
            <column name="SBI_VERSION_DE" />
        </property>
        <property name="metaVersion" type="string">
            <column name="META_VERSION" />
        </property>
        <property name="organization" type="string">
            <column name="ORGANIZATION" />
        </property>
        <property name="timeIn" type="timestamp">
            <column name="TIME_IN" not-null="true" />
        </property>
        <property name="timeUp" type="timestamp">
            <column name="TIME_UP" />
        </property>
        <property name="timeDe" type="timestamp">
            <column name="TIME_DE" />
        </property>
    </class>
</hibernate-mapping>
